#include <iostream>
#include<string>
using namespace std;

int main() {
    char str[20];
    cout<<"Enter a string: ";
    cin>>str;
    char *ptr=str;
   
    cout<<"String is: ";
    while( *ptr !='\0'){
        cout<<*ptr;
        ptr++;
    }
    return 0;
}

// output
Enter a string: helo
String is: helo
